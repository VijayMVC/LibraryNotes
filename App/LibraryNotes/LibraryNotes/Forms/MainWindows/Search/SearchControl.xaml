<UserControl x:Class="LibraryNotes.Forms.MainWindows.Search.SearchControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LibraryNotes.Forms.MainWindows.Search"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Height="640" Width="800">
    <Grid>
        <Grid>
            <TabControl>
                <TabItem Header ="Similar books"  Name="Content_similar_book"></TabItem>
                <TabItem Header ="By genres" Name="Content_genres"></TabItem>
                <TabItem Header ="By authors" Name="Content_authors"></TabItem>
            </TabControl>
        </Grid>
    </Grid>
</UserControl>
